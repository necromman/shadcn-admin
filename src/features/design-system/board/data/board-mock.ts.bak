// ê²Œì‹œíŒë³„ ê²Œì‹œê¸€ í•„í„°ë§
export const getPostsByBoardId = (boardId: string): Post[] => {
  return mockPosts.filter(post => post.boardId === boardId)
}

// ê³ ì • ê²Œì‹œê¸€ ê°€ì ¸ì˜¤ê¸°
export const getPinnedPosts = (boardId?: string): Post[] => {
  const posts = boardId 
    ? mockPosts.filter(post => post.boardId === boardId) 
    : mockPosts
  return posts.filter(post => post.isPinned)
}

// ì¸ê¸° ê²Œì‹œê¸€ ê°€ì ¸ì˜¤ê¸°
export const getPopularPosts = (limit = 5): Post[] => {
  return [...mockPosts]
    .sort((a, b) => b.metadata.likes - a.metadata.likes)
    .slice(0, limit)
}

// ìµœì‹  ê²Œì‹œê¸€ ê°€ì ¸ì˜¤ê¸°
export const getRecentPosts = (limit = 5): Post[] => {
  return [...mockPosts]
    .sort((a, b) => new Date(b.createdAt).getTime() - new Date(a.createdAt).getTime())
    .slice(0, limit)
}